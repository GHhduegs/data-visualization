# Task 6: Graph/Network Visualization
import networkx as nx
from pyvis.network import Network

G = nx.Graph()
G.add_edges_from([("A", "B"), ("A", "C"), ("B", "D"), ("C", "D")])

# Force Layout (PyVis)
net = Network(height="400px", width="600px", notebook=True)
net.from_nx(G)
net.show("graph_network.html")
